  "description": "A job board app made with React",
  "main": "./src/index",
  "scripts": {
    "start": "node -r dotenv/config bin/www -w app.js -w server",
    "server": "set NODE_ENV=development&& nodemon  bin/www",
    "test": "mocha  --require ts-node/register src/**/*.spec.ts",
    "build": "webpack --env.dev=prod",
    "build:dev": "set NODE_ENV=development webpack --env.dev --hot",
    "dev": "webpack-dev-server --env.dev=development --history-api-fallback --port 3000 --hot --host 0.0.0.0",
    "devprod": "webpack-dev-server --env.dev=prod --history-api-fallback --port 3000 --hot --host 0.0.0.0"
  },
